{
  "name": "cs-activiam-activeui",
  "version": "3.1.0",
  "description": "a test npm package",
  "repository": "git@github.com:PChol22/cs-activiam-activeui.git",
  "author": "Pchol22 <pierre.chollet@student-cs.fr>",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^17.0.37",
    "react-dom": "^17.0.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "yarn build:clean && yarn build:esm && yarn build:cjs && yarn build:css",
    "build:clean": "rm -rf lib",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:css": "cp src/main.css lib/esm/main.css && cp src/main.css lib/cjs/main.css",
    "style": "npx prettier . --write && yarn run eslint ./src/**/*.tsx"
  },
  "main": "./lib/cjs/main.js",
  "module": "./lib/esm/main.js",
  "types": "./lib/esm/main.d.ts",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/eslint": "^8.2.1"
  }
}
